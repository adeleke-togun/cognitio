.container
  section#home.main(is-authorized ng-show="showPage")
    include ../../shared/admin_stamp
    include ../../shared/notification
    .container-fluid
      .row
      .menu.row
        .submit.col-md-6.submitAssignment
          .options.submit
            .text-center.submitIcon
              img(src="../img/feedback-100.png")
            .text-center.submitClick
              a.submitBtn.btn.btn-default.text-center(href='/admin/create-assessment') Create Assessment
        .view.col-md-6.viewAssignment
          .options.view
            .text-center.submitIcon
              img(src="../img/report_card-100.png")
            .text-center.submitClick
              a.submitBtn.btn.btn-default.btn-lg.text-center(href='/admin/view-assessments') View Assessments